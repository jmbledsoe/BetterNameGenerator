<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5.1" />
      <httpRuntime targetFramework="4.5.1" />
    </system.web>

  <system.webServer>    
    <rewrite>
      <rules>
        <rule name="result" enabled="true" stopProcessing="true">
          <match url="result/(male|female)/+"/>
          <action type="Rewrite" url="/index.html"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>

</configuration>
